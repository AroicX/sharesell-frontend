{
  "moduleResolution": "node",
  "compilerOptions": {
    "jsx": "react",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@/assets/*": ["/assets/*"],
      "@/components/*": ["/components/*"],
      "@/hooks/*": ["/hooks/*"],
      "@/services/*": ["/services/*"],
      "@/helpers/*": ["/helpers/*"],
      "@/reusable/*": ["/reusable/*"],
      "@/styles/*": ["/reusable/*"]
    }
  },
  "exclude": ["node_modules", ".next"]
}
